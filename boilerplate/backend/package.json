{
  "name": "backend",
  "version": "1.0.0",
  "description": "word checker API server",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --fix \"**/*.ts\"",
    "prettier": "prettier --check \"**/*.ts\"",
    "prettier:fix": "prettier --write \"**/*.ts\"",
    "test": "mocha --timeout 30000 --parallel --reporter list --require ts-node/register --require tsconfig-paths/register 'src/**/*.spec.ts'",
    "test:e2e": "mocha --timeout 30000 --parallel --reporter list --require ts-node/register --require tsconfig-paths/register 'test/**/*spec.ts'"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "@nestjs/common": "^10.2.1",
    "@nestjs/core": "^10.2.1",
    "@nestjs/platform-fastify": "^10.2.1",
    "@nestjs/swagger": "^7.1.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.9",
    "fastify-metrics": "^10.3.0",
    "lodash": "^4.17.21",
    "mongodb": "^5.9.1",
    "nanoid": "^3.3.6",
    "prom-client": "^15.1.0",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.10.0",
    "winston-transport": "^4.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.1",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/lodash": "^4.14.197",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.11.18",
    "@types/qs": "^6.9.7",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.5",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "mocha": "^10.2.0",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "sinon": "^17.0.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  }
}
