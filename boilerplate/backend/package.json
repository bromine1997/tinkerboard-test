{
  "name": "backend",
  "version": "1.0.0",
  "description": "API server",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --fix \"**/*.ts\"",
    "prettier": "prettier --check \"**/*.ts\"",
    "prettier:fix": "prettier --write \"**/*.ts\"",
    "test": "mocha --timeout 30000 --parallel --reporter list --require ts-node/register --require tsconfig-paths/register 'src/**/*.spec.ts'",
    "test:e2e": "mocha --timeout 30000 --parallel --reporter list --require ts-node/register --require tsconfig-paths/register 'test/**/*spec.ts'"
  },
  "dependencies": {
    "@fastify/static": "^7.0.1",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.2.1",
    "@nestjs/platform-socket.io": "^10.4.1",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/websockets": "^10.4.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.9",
    "express": "^4.21.0",
    "fastify-metrics": "^10.3.0",
    "lodash": "^4.17.21",
    "mongodb": "^5.9.1",
    "mongoose": "^8.6.3",
    "nanoid": "^3.3.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prom-client": "^15.1.0",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "triple-beam": "^1.4.1",
    "webidl-conversions": "^7.0.0",
    "winston": "^3.10.0",
    "winston-transport": "^4.7.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.1",
    "@types/chai": "^4.3.19",
    "@types/lodash": "^4.14.197",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.50",
    "@types/qs": "^6.9.7",
    "@types/sinon": "^17.0.3",
    "@types/triple-beam": "^1.3.5",
    "@types/validator": "^13.12.1",
    "@types/webidl-conversions": "^7.0.3",
    "@types/whatwg-url": "^11.0.5",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "chai": "^5.1.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "mocha": "^10.4.0",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "sinon": "^17.0.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
