{
  "name": "backend",
  "version": "1.0.0",
  "description": "word checker API server",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --fix \"**/*.ts\"",
    "prettier": "prettier --check \"**/*.ts\"",
    "prettier:fix": "prettier --write \"**/*.ts\"",
    "test": "mocha --timeout 30000 --parallel --reporter list --require ts-node/register --require tsconfig-paths/register 'src/**/*.spec.ts'",
    "test:e2e": "mocha --timeout 30000 --parallel --reporter list --require ts-node/register --require tsconfig-paths/register 'test/**/*spec.ts'"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.2",
    "@nestjs/core": "^9.0.2",
    "@nestjs/platform-fastify": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "fastify-metrics": "^10.3.0",
    "fastify-swagger": "^5.2.0",
    "lodash": "^4.17.21",
    "mongodb": "^5.2.0",
    "nanoid": "^4.0.2",
    "qa": "^0.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^10.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "18.11.18",
    "@types/qs": "^6.9.7",
    "@types/sinon": "^10.0.14",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10.2.0",
    "prettier": "^2.3.2",
    "rimraf": "^5.0.0",
    "sinon": "^15.0.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.7.4"
  }
}
